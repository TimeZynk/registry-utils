{
    "name": "timezynk-registry-utils",
    "type": "module",
    "version": "0.7.4",
    "description": "Utilities for working with Timezynk registries.",
    "repository": {
        "type": "git",
        "url": "https://github.com/TimeZynk/registry-utils"
    },
    "files": [
        "lib"
    ],
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "scripts": {
        "prepare": "npm run build",
        "preversion": "npm run lint",
        "version": "npm run format && git add -A src",
        "postversion": "git push && git push --tags",
        "format": "prettier --write \"src/**/*.ts\"",
        "test": "TZ=UTC vitest run",
        "test:watch": "TZ=UTC vitest",
        "build": "tsc",
        "lint": "eslint src"
    },
    "dependencies": {
        "dateformat-light": "^1.3.5",
        "immutable": "^3.8.2",
        "lodash-es": "^4.17.21",
        "reselect": "^4.1.6",
        "tzdateutils": "^1.4.5"
    },
    "devDependencies": {
        "@eslint/js": "^9.34.0",
        "@types/lodash": "^4.17.20",
        "@types/lodash-es": "^4.17.12",
        "@types/node": "^22.17.2",
        "@typescript-eslint/eslint-plugin": "^8.40.0",
        "@typescript-eslint/parser": "^8.40.0",
        "@vitest/ui": "^3.2.4",
        "babel-eslint": "^10.1.0",
        "eslint": "^8.57.1",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-prettier": "^5.5.4",
        "eslint-plugin-vitest-globals": "^1.5.0",
        "globals": "^16.3.0",
        "prettier": "^3.6.2",
        "typescript": "^5.9.2",
        "vitest": "^3.2.4"
    },
    "engines": {
        "node": ">=20.0.0",
        "npm": ">=10.0.0"
    },
    "author": "Timezynk AB",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/TimeZynk/registry-utils/issues"
    },
    "homepage": "https://github.com/TimeZynk/registry-utils#readme",
    "publishConfig": {
        "registry": "https://registry.npmjs.org/",
        "provenance": "true"
    }
}
